{
  "name": "storefront_api",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "test": "jasmine",
    "lint": "eslint .",
    "clean": "shx rm -rf build",
    "prebuild": "npm run clean",
    "lint:fix": "eslint . --fix",
    "migrate:up": "db-migrate up",
    "start": "node build/server.ts",
    "seed": "node scripts/seedup.js",
    "migrate:down": "db-migrate down",
    "teardown": "node scripts/teardown.js",
    "format": "prettier \"src/**/*.ts\" --write",
    "dev": "tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess \"node ./build/server.js\""
  },
  "keywords": [
    "Udacity",
    "Egypt FWD",
    "Storefront",
    "API",
    "Node",
    "TypeScript",
    "Jasmine",
    "Unit",
    "Testing",
    "ESLint",
    "Prettier"
  ],
  "author": {
    "name": "Ahmed Ismail Abu Qahf",
    "email": "ahmedsomaa@aucegypt.edu",
    "url": "https://github.com/ahmedsomaa"
  },
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/jasmine": "4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.8.0",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "4.4.0",
    "jasmine-spec-reporter": "7.0.0",
